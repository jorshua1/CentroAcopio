<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="--textLight" Color="#0f0c00"/>
    <SolidColorBrush x:Key="--backgroundLight" Color="#e2f0f3"/>
    <SolidColorBrush x:Key="--primaryLight" Color="#33ff7a"/>
    <SolidColorBrush x:Key="--secondaryLight" Color="#1f62ff"/>
    <SolidColorBrush x:Key="--accentLight" Color="#dab41b"/>
    <SolidColorBrush x:Key="--textDark" Color="#fffcf0"/>
    <SolidColorBrush x:Key="--backgroundDark" Color="#0c1a1d"/>
    <SolidColorBrush x:Key="--primaryDark" Color="#00cc47"/>
    <SolidColorBrush x:Key="--secondaryDark" Color="#0043e0"/>
    <SolidColorBrush x:Key="--accentDark" Color="#e4be25"/>
    
</ResourceDictionary>